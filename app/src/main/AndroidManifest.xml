<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zhuoxin.lineng.mymusicplays">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/kugou"
        android:label="小酷狗音乐"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Activity.Activity.Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Activity.Activity.titleactivity">

        </activity>
        <activity android:name=".Activity.Activity.Activity.Activity_play">

        </activity>
        <service android:name=".Activity.Activity.Activity.service.MusicService"></service>
    </application>

</manifest>